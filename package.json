{
  "name": "nuxtjs-project",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint \"./src/**/*.{ts,vue}\"",
    "lint:fix": "eslint --fix \"./src/**/*.{ts,vue}\"",
    "format:check": "prettier --check \"./**/*.{vue,ts}\"",
    "format": "prettier --write \"./**/*.{vue,ts}\" && yarn lint:fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@vue/composition-api": "^1.1.3",
    "core-js": "^3.15.1",
    "nuxt": "^2.15.7"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@nuxt/types": "^2.15.7",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "fibers": "^5.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "sass": "^1.38.1",
    "sass-loader": "10"
  },
  "lint-staged": {
    "*.ts": "tsc --pretty",
    "*.{vue,ts}": [
      "prettier --write",
      "eslint --cache --fix \"./src/**/*.{ts,vue}\""
    ]
  }
}
